{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "scripts": {
    "start": "fastify start -l info dist/app.js",
    "build": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l trace -P dist/app.js\""
  },
  "dependencies": {
    "@getf1tickets/sdk": "file:.yalc/@getf1tickets/sdk",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.14.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.5"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^15.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getf1tickets/auth-service.git"
  },
